Type: Package
Package: AMAPVox
Title: LiDAR Data Voxelisation
Version: 0.10.0
Authors@R: c(
    person("Grégoire", "Vincent", , "gregoire.vincent@ird.fr", role = "aut"),
    person("Julien", "Heurtebize", , "julienhtbe@gmail.com", role = "aut"),
    person("Philippe", "Verley", , "philippe.verley@ird.fr", role = c("aut", "cre"))
